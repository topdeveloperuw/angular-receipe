{
  "extends": "airbnb/legacy",

  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },

  "globals": {
    "angular"
  },

  "rules": {
    "vars-on-top": 0,
    "no-unused-vars": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0
  }
}
